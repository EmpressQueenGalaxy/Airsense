{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/index.js"
    },
    {
      "src": "/",
      "dest": "/public/visor.html"
    },
    {
      "src": "/(.*)\\.(css|js|png|jpg|svg|json|ico)$",
      "dest": "/public/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
