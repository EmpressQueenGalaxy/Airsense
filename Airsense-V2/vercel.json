{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/backend/index.js" },
    { "src": "/(js|images|css)/(.*)", "dest": "/public/$1/$2" },
    { "src": "/(.*\\.css)", "dest": "/public/$1" },
    { "src": "/(.*\\.js)", "dest": "/public/$1" },
    { "src": "/", "dest": "/public/visor.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
